    "size-limit": "^4.10.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@prisma/client": "^2.20.1",
    "@prisma/generator-helper": "^2.20.1",
    "listr": "^0.14.3",
