datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

enum UserType {
  USER
  ADMIN
}

/// user type comment
enum UserTypeWithDoc {
  USER
  ADMIN
}

model User {
  id    Int      @id @default(autoincrement())
  email String   @unique
  name  String?
  type  UserType
}
